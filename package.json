{
  "name": "devlist",
  "version": "0.0.1",
  "private": true,
  "description": "A community driven list of tools for developers.",
  "main": "index.js",
  "author": "Jordan Walker",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "3.5.1",
    "jest": "^22.4.3",
    "react-scripts": "1.1.1"
  },
  "dependencies": {
    "autoprefixer-stylus": "0.14.0",
    "firebase": "^4.12.1",
    "prop-types": "^15.6.0",
    "re-base": "3.2.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.0",
    "serve": "^6.5.0",
    "stylus": "0.54.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch":
      "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "styles":
      "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch":
      "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  }
}
